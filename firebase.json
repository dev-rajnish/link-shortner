{
  "hosting": {
    "public": "public",
    "site": "rsh",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/github",
        "destination": "https://github.com/dev-rajnish",
        "type": 301
      },
      {
      "source" : "index",
      "destination" : "/index.html",
"type": 301
      },
      {
      "source" : "tpt",
      "destination" : "https://raw.githubusercontent.com/dev-rajnish/bash-exec/master/terminal-GPT/install.sh",
"type": 301
      },
      {
      "source" : "f2g",
      "destination" : "https://raw.githubusercontent.com/dev-rajnish/bash-exec/master/folder-to-github/install.sh",
"type": 301
      }
    ]
  }
}
